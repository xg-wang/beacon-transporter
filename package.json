{
  "name": "beacon-transporter",
  "version": "0.6.0",
  "description": "Reliable beacon API for the browser",
  "license": "MIT",
  "author": "Thomas Wang <hi@xgwang.me>",
  "homepage": "https://github.com/xg-wang/beacon-transporter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xg-wang/beacon-transporter.git"
  },
  "files": [
    "src",
    "dist",
    "!dist/**/*.d.ts.map",
    "docs"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "browser": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "node demo/server.cjs",
    "docs": "api-extractor run --local --verbose && api-documenter markdown -i ./temp -o ./docs",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:tsc-src": "tsc --noEmit",
    "prepare": "tsc && rollup -c rollup.config.js && npm run docs",
    "test": "npm-run-all prepare lint jest",
    "jest": "jest"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.24",
    "@microsoft/api-extractor": "^7.16.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "body-parser": "^1.19.0",
    "create-test-server": "^3.0.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.17.1",
    "idb-queue": "^0.1.5",
    "jest": "^27.0.5",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.12.3",
    "prettier": "^2.3.2",
    "rollup": "^2.52.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "wait-for-expect": "^3.0.2"
  },
  "volta": {
    "node": "16.1.0"
  }
